{
	"expo": {
		"name": "quick-actions",
		"slug": "quick-actions",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "quickactions",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#000"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-quick-actions",
				{
					"androidIcons": {
						"help_icon": {
							"foregroundImage": "./assets/images/help-icon.png"
						},
						"about_icon": {
							"foregroundImage": "./assets/images/about-icon.png"
						}
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
